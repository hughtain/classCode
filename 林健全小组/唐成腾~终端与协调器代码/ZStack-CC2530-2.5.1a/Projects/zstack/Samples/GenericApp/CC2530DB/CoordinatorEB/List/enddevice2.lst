    380                }
    381          #endif
    382                break;
    383          
    384              case Match_Desc_rsp:
    385                {
    386                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    387                  if ( pRsp )
